

<Blazored.Modal.CascadingBlazoredModal HideCloseButton="true" DisableBackgroundCancel="true" HideHeader="true" Class="blazored-custom-modal">
<Notes2022RCL.Comp.CookieStateAgent></Notes2022RCL.Comp.CookieStateAgent>

<Router AppAssembly="@typeof(Main).Assembly">
	<Found Context="routeData">
		<RouteView RouteData="@routeData" DefaultLayout="@typeof(Notes2022RCL.Shared.MainLayout)" />
        <FocusOnNavigate RouteData="@routeData" Selector="h1" />
	</Found>
	<NotFound>
		<LayoutView Layout="@typeof(Notes2022RCL.Shared.MainLayout)">
			<p role="alert">Sorry.................., there's nothing at this address.</p>
		</LayoutView>
	</NotFound>
</Router>
</Blazored.Modal.CascadingBlazoredModal>